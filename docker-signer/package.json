{
  "name": "verbitto-openclaw-skill",
  "version": "1.0.0",
  "description": "Verbitto signing proxy service for OpenClaw agents",
  "main": "verbitto-signer.js",
  "scripts": {
    "start": "node verbitto-signer.js",
    "start:bg": "nohup node verbitto-signer.js > signer.log 2>&1 &",
    "stop": "pkill -f verbitto-signer.js",
    "logs": "tail -f signer.log",
    "example": "bash example-agent.sh"
  },
  "keywords": [
    "verbitto",
    "openclaw",
    "solana",
    "ai-agent",
    "task-escrow"
  ],
  "author": "Verbitto Team",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.2",
    "@solana/web3.js": "^1.87.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
